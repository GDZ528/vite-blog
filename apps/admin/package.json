{
  "name": "admin",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "lint": "lint-staged",
    "eslint": "eslint --ext .js,.jsx,.ts,.tsx --fix --quiet ./"
  },
  "prettier": {
    "arrowParens": "always",
    "bracketSpacing": true,
    "printWidth": 120,
    "singleQuote": false,
    "tabWidth": 2,
    "trailingComma": "es5"
  },
  "lint-staged": {
    "*.{js,jsx,tsx,ts}": [
      "yarn eslint",
      "prettier --write"
    ],
    "*.{json,md,yaml}": [
      "prettier --write"
    ]
  },
  "dependencies": {
    "antd": "^4.23.0",
    "axios": "^0.27.2",
    "classnames": "^2.3.2",
    "i18next": "^22.4.15",
    "jsdom": "^21.1.1",
    "less": "^4.1.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-i18next": "^12.2.2",
    "vite-plugin-imp": "^2.3.0"
  },
  "devDependencies": {
    "@babel/core": "^7.19.1",
    "@types/react": "^18.0.17",
    "@types/react-dom": "^18.0.6",
    "@vitejs/plugin-react": "^2.0.1",
    "babel-loader": "^8.2.5",
    "eslint": "^8.23.0",
    "lint-staged": "^13.0.3",
    "prettier": "^2.7.1",
    "react-router-dom": "^6.3.0",
    "typescript": "^4.6.4",
    "vite": "^3.0.7",
    "vite-plugin-windicss": "^1.8.8",
    "windicss": "^3.5.6",
    "windi-config": "*",
    "ui": "*",
    "yhooks": "*",
    "public-utils": "*"
  }
}
