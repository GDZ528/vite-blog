  "main": "./index.ts",
  "types": "./index.ts",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "lint": "lint-staged",
    "test": "vitest --run",
    "coverage": "vitest run --coverage",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "eslint": "eslint --ext .js,.jsx,.ts,.tsx --fix --quiet ./"
  },
  "prettier": {
    "arrowParens": "always",
    "bracketSpacing": true,
    "printWidth": 120,
    "singleQuote": false,
    "tabWidth": 2,
    "trailingComma": "es5"
  },
  "lint-staged": {
    "*.{js,jsx,tsx,ts}": [
      "yarn eslint",
      "prettier --write"
    ],
    "*.{json,md,yaml}": [
      "prettier --write"
    ]
  },
  "dependencies": {
    "@testing-library/jest-dom": "^5.16.5",
    "antd": "^4.23.0",
    "classnames": "^2.3.2",
    "react": "^18.2.0",
